{
	"name": "frontend",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"build:docs-install": "cd ../docs-quartz && npm install",
		"build:docs-quartz": "cd ../docs-quartz && npx quartz build --output ../frontend/static/docs",
		"build:docs": "npm run build:docs-install && npm run build:docs-quartz",
		"build:with-docs": "npm run build:docs && vite build",
		"build:app": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@playwright/test": "^1.57.0",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@testing-library/svelte": "^5.2.8",
		"@vitest/coverage-v8": "^3.0.0",
		"jsdom": "^25.0.0",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"terser": "^5.44.1",
		"typescript": "^5.9.3",
		"vite": "^7.2.6",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@types/leaflet": "^1.9.21",
		"browser-image-compression": "^2.0.2",
		"chart.js": "^4.5.1",
		"leaflet": "^1.9.4",
		"leaflet.markercluster": "^1.5.3",
		"lucide-svelte": "^0.562.0"
	}
}
